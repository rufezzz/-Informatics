#include <fstream>

int main(void)
{
    std::ifstream in;
    std::ofstream out;

    in.open("input.txt");
    out.open("output.txt");

    int a;
    in>>a;

    int i = 1;
    
    while(i<a)
    {
        i=i*2;
    }
    if (i==a)
    {
        out<<"YES";
    }
    else
    {
        out<<"NO";
    }
    
    return 0;
}
